{
  "name": "vue-vkontakte-icons",
  "version": "3.0.0-beta.2",
  "type": "module",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ivanmem/vue-vkontakte-icons",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
