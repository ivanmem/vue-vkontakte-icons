{
  "name": "vue-vkontakte-icons",
  "version": "3.0.0-beta.9",
  "description": "A Vue 3 Vkontakte Icon Component Library.",
  "type": "module",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "author": "ivanmem",
  "license": "MIT",
  "files": [
    "dist/",
    "src/assets/",
    "volar.d.ts",
    "web-types.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanmem/vue-vkontakte-icons"
  },
  "keywords": [
    "vue-vkontakte-icons",
    "component library",
    "vue",
    "vkontakte"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/main.js"
    },
    "./assets/*": "./src/assets/*"
  },
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "generate-icons": "cross-env GENERATE_ICONS=true vite build"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/node": "18.18.2",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/tsconfig": "0.7.0",
    "cross-env": "7.0.3",
    "fs-extra": "^11.1.1",
    "path": "0.12.7",
    "typescript": "5.8.2",
    "vite": "6.2.4",
    "vite-plugin-dts": "4.5.3",
    "vite-svg-loader": "5.1.0",
    "vue": "3.5.13",
    "vue-tsc": "2.2.8",
    "es-toolkit": "1.34.1"
  },
  "peerDependencies": {
    "vue": ">=3.2.13"
  }
}
