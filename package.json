  },
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "generate-icons": "cross-env GENERATE_ICONS=true vite build"
  },
  "devDependencies": {
    "@types/node": "18.18.2",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/tsconfig": "0.7.0",
    "path": "0.12.7",
    "typescript": "5.8.2",
    "vite": "6.2.4",
    "vite-plugin-dts": "4.5.3",
    "vite-svg-loader": "5.1.0",
    "vue-tsc": "2.2.8",
    "cross-env": "7.0.3",
    "vue": "3.5.13"
  },
  "peerDependencies": {
    "vue": ">=3.2.13"
  }
}
